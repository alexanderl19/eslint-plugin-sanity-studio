{
  "name": "eslint-plugin-sanity-studio",
  "version": "0.4.0",
  "description": "Custom linting rules for Sanity Studio.",
  "repository": {
    "url": "git+https://github.com/alexanderl19/eslint-plugin-sanity-studio"
  },
  "keywords": [
    "eslint",
