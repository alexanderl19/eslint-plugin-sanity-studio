    "eslint-plugin-node": "11.1.0",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
